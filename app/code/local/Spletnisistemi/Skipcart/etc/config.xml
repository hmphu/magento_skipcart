<?xml version="1.0"?>
<config>
    <modules>
        <Spletnisistemi_Skipcart>
            <version>1.0</version>
        </Spletnisistemi_Skipcart>
    </modules>
    <frontend>
        <routers>
            <checkout>
                <args>
                    <modules>
                        <Spletnisistemi_Skipcart before="Mage_Checkout_CartController">Spletnisistemi_Skipcart
                        </Spletnisistemi_Skipcart>
                    </modules>
                </args>
            </checkout>
        </routers>
    </frontend>
    <default>
        <checkout>
            <skipcart>
                <skip_enabled>1</skip_enabled>
            </skipcart>
        </checkout>
    </default>
</config>